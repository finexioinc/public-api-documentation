          {
            "$ref": "#/components/schemas/CounterpartyBase"
          },
          {
            "type": "object",
            "properties": {
              "type": {
                "enum": [
                  "Supplier"
                ]
              }
            },
            "required": [
              "name",
              "type",
              "address_1",
              "city",
              "state",
